{
  "name": "pinto-meet",
  "version": "0.1.0",
  "engines": {
    "node": "14.x"
  },
  "website": "https://meet.pintopinto.org/",
  "logo": "/favicon.ico",
  "scripts": {
    "build": "next build",
    "start": "next start",
    "dev": "next dev -p 5000",
    "prod": "npx vercel dev --listen 5000 --debug",
    "pretty-quick": "pretty-quick",
    "lint": "eslint --ignore-path .gitignore --ext .ts,.tsx .",
    "test": "node ./test/server.js --trace-warnings",
    "prepare": "husky install"
  },
  "dependencies": {
    "autoprefixer": "^10.2.4",
    "haikunator": "^2.1.2",
    "next": "^10.1.3",
    "peerjs": "^1.3.2",
    "postcss": "^8.2.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "socket.io-client": "^3.1.2",
    "tailwindcss": "^2.1.1",
    "twilio": "^3.60.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged && npx pretty-quick --staged",
      "pre-push": "npx tsc && npm run lint"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@vercel/node": "^1.9.1",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "express": "^4.17.1",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "2.2.1",
    "pretty-quick": "^3.1.0",
    "typescript": "^4.2.4",
    "vercel": "^21.3.3"
  },
  "author": "David Solis",
  "repository": "https://github.com/pinto-pinto/pinto-meet",
  "description": "Video conferencing for the rest of us, join a room to get started today.",
  "keywords": [
    "FaceTime",
    "Zoom",
    "Video Conferencing",
    "Meet",
    "WebRTC",
    "TailwindCSS",
    "Next.js"
  ],
  "license": "GPL-3.0-or-later",
  "private": true
}
